title = {{Computing needs time}},
volume = {52},
year = {2009}
}
@misc{Pervasive2008,
abstract = {This note provides an overview of pervasive computing (PvC) and discusses the growing debate over privacy, safety and environmental implications. It mentions that these implication are a result of their ability to: (a) gather sensitive data, for example on users' everyday interactions, movements, preferences and attitudes, without user intervention or consent; (b) retrieve and use information from large databases/archives of stored data; (c) alter the environment via actuating devices. Re. privacy it mentions opportunities for data interception, theft and ubiquitous surveillance. Also discusses that because data mining activities can detect unknown relationships in data, there is the potential to violate existing legislation. Such concerns may need to be addressed by means of voluntary guidelines, legislative measures, physical design, or a combination of these.},
author = {Pervasive, Background},
booktitle = {Pervasive and Mobile Computing},
doi = {10.1016/j.pmcj.2008.05.001},
